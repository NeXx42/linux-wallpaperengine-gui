<UserControl xmlns="https://github.com/avaloniaui"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
  xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450" x:Class="AvaloniaUI.Pages._HomePage.WallpaperProperties.HomePage_WallpaperProperties_Bool">
  <Grid ColumnDefinitions="100, *" ColumnSpacing="15" Height="50">
    <Label x:Name="lbl" Content="Offset Y" VerticalAlignment="Center" FontSize="12"/>
    <ToggleButton Grid.Column="1" HorizontalAlignment="Stretch" x:Name="inp">
      <ToggleButton.Styles>
        <Style Selector="ToggleButton">
          <Setter Property="Content" Value="Disabled"/>
        </Style>
        <Style Selector="ToggleButton:checked">
          <Setter Property="Content" Value="Enabled"/>
        </Style>
      </ToggleButton.Styles>
    </ToggleButton>
  </Grid>
</UserControl>
