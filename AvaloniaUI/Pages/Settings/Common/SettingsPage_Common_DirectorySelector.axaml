<UserControl xmlns="https://github.com/avaloniaui"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
  xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="50" x:Class="AvaloniaUI.Pages.Settings.Common.SettingsPage_Common_DirectorySelector"
  xmlns:Common="clr-namespace:AvaloniaUI.Common">
  <Grid ColumnDefinitions="400, *, 100" ColumnSpacing="5" Margin="5">
    <Label Content="Startup script" VerticalAlignment="Center" x:Name="lbl_Loc"/>
    <Common:Common_Button Grid.Column="1" Label="Select Directory" HorizontalAlignment="Stretch" x:Name="btn_Select"/>
    <Common:Common_Button Grid.Column="2" Label="Reset" HorizontalAlignment="Stretch" x:Name="btn_Reset"/>
  </Grid>
</UserControl>
